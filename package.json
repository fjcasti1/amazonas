  },
  "homepage": "https://github.com/fjcasti1/amazonas#readme",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.2.0",
    "@stripe/stripe-js": "^1.11.0",
    "aws-sdk": "^2.828.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "colors": "^1.4.0",
    "country-list": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
